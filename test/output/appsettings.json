{
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "webApi": {
        "Protocols": "Http1",
        "Url": "http://localhost:5000"
      },
      "Grpc": {
        "Protocols": "Http2",
        "Url": "http://localhost:5001"
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  }
}
